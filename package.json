{
  "name": "atlas",
  "version": "0.0.10",
  "license": "Nuvasive",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot --build-optimizer=false --open --watch --proxy-config proxy.config.json",
    "build": "ng build --aot --prod --stats-json --build-optimizer --vendor-chunk --extract-css=false --named-chunks=false",
    "test": "ng test --browser ChromeHeadless --single-run",
    "watch": "ng test --browser ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "./node_modules/.bin/firebase deploy",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "presme": "ng build --aot --prod --sourcemaps --build-optimizer --vendor-chunk --extract-css=false --named-chunks=true --output-hashing=none",
    "sme": "source-map-explorer ./dist/vendor.bundle.js",
    "i18n": "ng xi18n --output-path=./src/assets/i18n/ --out-file=messages.no.xlf",
    "doc": "./node_modules/.bin/compodoc -p tsconfig.json -s"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/cdk": "^5.0.0-rc0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/material": "^5.0.0-rc0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@compodoc/compodoc": "^1.0.3",
    "@ngx-translate/core": "^8.0.0",
    "@ngx-translate/http-loader": "^2.0.0",
    "angular2-notifications": "latest",
    "core-js": "^2.4.1",
    "flag-icon-css": "^2.8.0",
    "lodash-es": "^4.17.4",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "next",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@angular/service-worker": "^5.0.0",
    "@types/core-js": "^0.9.43",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/lodash-es": "^4.14.7",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.1.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.0",
    "webpack-bundle-analyzer": "^2.9.0"
  },
  "optionalDependencies": {
    "firebase-tools": "^3.9.2"
  }
}
